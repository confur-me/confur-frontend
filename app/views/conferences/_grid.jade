.grid.conferences-list.row
  .col-xs-12.col-sm-4.col-md-4.col-lg-3(ng-repeat="conference in conferences track by conference.slug")
    .item.conference-item(bindonce="conference")
      a(bo-href="'/conferences/' + conference.slug")
        .thumbnail(bo-html="conference.thumbnailTag()")
      .details
        h4(bo-text="conference.title")
